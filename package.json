{
  "name": "my-game",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Nikita Prodan <sa_vvk@mail.ru>",
  "description": "Pet project that repeating a famous television game",
  "keywords": [
    "game",
    "questions"
  ],
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerdych/My-game.git"
  },
  "homepage": "https://github.com/Nerdych/My-game#readme",
  "bugs": "https://github.com/Nerdych/My-game/issues",
  "scripts": {
    "start": "webpack serve --env port=3000 bundleAnalyzerPort=4000",
    "build:dev": "webpack --config webpack.config.ts --env mode=development",
    "build:prod": "webpack --config webpack.config.ts --env mode=production",
    "lint:ts": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:ts:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "lint:scss": "stylelint src/**/*.{scss,css}",
    "lint:scss:fix": "stylelint --fix src/**/*.{scss,css}",
    "prettier": "prettier --check src/**/*",
    "prettier:fix": "prettier --write src/**/*",
    "test:unit": "jest --config config/jest/jest.config.ts",
    "cypress:open": "cypress open --config-file config/cypress/cypress.config.ts",
    "autotest:open": "concurrently \"npm run start\" \"npm run cypress:open\"",
    "autotest:run": "cypress run --config-file config/cypress/cypress.config.ts"
  },
  "dependencies": {
    "i18next": "23.6.0",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "13.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.7",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/webpack": "5.28.2",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.2",
    "css-loader": "6.8.1",
    "cypress": "13.5.0",
    "eslint": "8.49.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-react": "7.33.2",
    "html-webpack-plugin": "5.5.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "mini-css-extract-plugin": "2.7.6",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "stylelint": "15.11.0",
    "stylelint-config-standard-scss": "11.0.0",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  }
}
