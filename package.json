    "html-webpack-plugin": "5.5.3",
    "mini-css-extract-plugin": "2.7.6",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "stylelint": "15.11.0",
    "stylelint-config-standard-scss": "11.0.0",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
