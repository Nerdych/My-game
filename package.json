    "react-dom": "18.2.0",
    "react-i18next": "13.3.1"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/webpack": "5.28.2",
